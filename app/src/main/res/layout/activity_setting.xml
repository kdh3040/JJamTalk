<?xml version="1.0" encoding="utf-8"?>


<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout6"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="20dp"
    >


    <ImageView
        android:id="@+id/imageView25"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginLeft="16dp"
        android:elevation="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_alarm"

        app:layout_constraintLeft_toLeftOf="@id/btn_alarm"

        app:layout_constraintTop_toTopOf="@id/btn_alarm"
        app:srcCompat="@drawable/alarm" />

    <Button
        android:id="@+id/btn_alarm"


        android:layout_width="0dp"
        android:layout_height="0dp"

        android:layout_margin="4dp"
        android:gravity="center_vertical"
        android:paddingLeft="70dp"
        android:text="알림 설정"
        android:textSize="14sp"
        android:textStyle="bold"
        android:background="@drawable/purple_stroke_btn_bg"
        app:layout_constraintBottom_toTopOf="@id/recvMessage"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginLeft="16dp"
        android:elevation="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/recvMessage"

        app:layout_constraintLeft_toLeftOf="@+id/recvMessage"

        app:layout_constraintTop_toTopOf="@+id/recvMessage"
        app:srcCompat="@drawable/hand" />

    <Button

        android:id="@+id/recvMessage"
        android:layout_width="0dp"
        android:layout_height="0dp"

        android:layout_margin="4dp"
        android:gravity="center_vertical"
        android:paddingLeft="70dp"
        android:text="@string/recvMessage"
        android:textSize="14sp"
        android:textStyle="bold"
        android:background="@drawable/purple_stroke_btn_bg"
        app:layout_constraintBottom_toTopOf="@+id/blocklist"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_alarm" />


    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginLeft="16dp"
        android:elevation="10dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="@+id/blocklist"

        app:layout_constraintLeft_toLeftOf="@+id/blocklist"

        app:layout_constraintTop_toTopOf="@+id/blocklist"
        app:srcCompat="@drawable/block" />

    <Button
        android:id="@+id/blocklist"

        android:layout_width="0dp"
        android:layout_height="0dp"


        android:layout_margin="4dp"
        android:gravity="center_vertical"
        android:paddingLeft="70dp"
        android:text="차단 목록 관리하기"
        android:textSize="14sp"
        android:textStyle="bold"
        android:background="@drawable/purple_stroke_btn_bg"
        app:layout_constraintBottom_toTopOf="@+id/btn_share"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/recvMessage" />


    <ImageView
        android:id="@+id/imageView28"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginLeft="16dp"
        android:elevation="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_share"

        app:layout_constraintLeft_toLeftOf="@+id/btn_share"

        app:layout_constraintTop_toTopOf="@+id/btn_share"
        app:srcCompat="@drawable/share" />

    <Button
        android:id="@+id/btn_share"

        android:layout_width="0dp"
        android:layout_height="0dp"

        android:layout_margin="4dp"
        android:gravity="center_vertical"
        android:paddingLeft="70dp"
        android:text="톡킹 공유하기"
        android:textSize="14sp"
        android:textStyle="bold"
        android:background="@drawable/purple_stroke_btn_bg"
        app:layout_constraintBottom_toTopOf="@+id/buyCoin"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/blocklist" />


    <ImageView
        android:id="@+id/imageView30"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginLeft="16dp"
        android:elevation="10dp"
        android:scaleType="fitCenter"

        app:layout_constraintBottom_toBottomOf="@+id/buyCoin"

        app:layout_constraintLeft_toLeftOf="@+id/buyCoin"
        app:layout_constraintTop_toTopOf="@+id/buyCoin"
        app:srcCompat="@drawable/coinbuy" />

    <Button
        android:id="@+id/buyCoin"

        android:layout_width="0dp"
        android:layout_height="0dp"

        android:layout_margin="4dp"
        android:gravity="center_vertical"
        android:paddingLeft="70dp"
        android:text="골드 구입하기"
        android:textSize="14sp"
        android:textStyle="bold"
        android:background="@drawable/purple_stroke_btn_bg"
        app:layout_constraintBottom_toTopOf="@+id/doc_law"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_share" />


    <ImageView
        android:id="@+id/imageView27"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginLeft="16dp"
        android:elevation="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/doc_law"

        app:layout_constraintLeft_toLeftOf="@+id/doc_law"

        app:layout_constraintTop_toTopOf="@+id/doc_law"
        app:srcCompat="@drawable/law" />

    <Button
        android:id="@+id/doc_law"

        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="4dp"
        android:gravity="center_vertical"

        android:paddingLeft="70dp"
        android:text="법적 고지"
        android:textSize="14sp"
        android:textStyle="bold"
        android:background="@drawable/purple_stroke_btn_bg"
        app:layout_constraintBottom_toTopOf="@+id/btn_help"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/buyCoin" />


    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginLeft="16dp"
        android:elevation="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_help"

        app:layout_constraintLeft_toLeftOf="@+id/btn_help"

        app:layout_constraintTop_toTopOf="@+id/btn_help"
        app:srcCompat="@drawable/help" />

    <Button
        android:id="@+id/btn_help"

        android:layout_width="0dp"
        android:layout_height="0dp"

        android:layout_margin="4dp"
        android:gravity="center_vertical"
        android:paddingLeft="70dp"
        android:text="도움말 및 고객 지원"
        android:textSize="14sp"
        android:textStyle="bold"
        android:background="@drawable/purple_stroke_btn_bg"
        app:layout_constraintBottom_toTopOf="@+id/btn_del"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/doc_law" />


    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginLeft="16dp"
        android:elevation="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_del"

        app:layout_constraintLeft_toLeftOf="@+id/btn_del"

        app:layout_constraintTop_toTopOf="@+id/btn_del"
        app:srcCompat="@drawable/delete" />

    <Button
        android:id="@+id/btn_del"


        android:layout_width="0dp"
        android:layout_height="0dp"

        android:layout_margin="4dp"
        android:gravity="center_vertical"
        android:paddingLeft="70dp"
        android:text="계정 삭제"
        android:textSize="14sp"
        android:textStyle="bold"
        android:background="@drawable/purple_stroke_btn_bg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_help" />


</android.support.constraint.ConstraintLayout>
